            <span>{% render 'smi-icons', icon: 'star' %}</span>
        {%- endif -%}
    {% endfor %}
</div>
    </div>
{%- endif -%}
        </div>
        {%- if heading != '' -%}
            <h4 class="{{ block.settings.heading_size }} smi-mb-xs smi-item__title smi-color-text smi-link-wrapper">{{- heading -}}</h4>
        {%- endif -%}
        {%- if description != '' -%}
            <div class="smi-text-md smi-color-text-75 smi-mb-0 smi-item__description smi-link-wrapper">{{- description -}}</div>
        {%- endif -%}
        </div>
        {%- if product != bank -%}
            <div class="smi-item__product smi-mt-lg smi-pt-lg smi-d-flex smi-align-items-center smi-column-gap-sm">
                {%- if product.featured_media -%}
                    <a href="{{product.url}}" target="_blank" class="smi-product-image smi-media-wrapper smi-media-settings smi-w-50px smi-h-50px">
