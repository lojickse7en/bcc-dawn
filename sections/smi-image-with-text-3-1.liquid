{% comment %}#component#["settings/heading","settings/media-settings-3","settings/layout-settings","settings/section-padding","blocks/background","settings/design-mode","settings/animation-settings","settings/rating","settings/slider-pagination-1-style"]#endcomponent#{% endcomment %}
{% comment %} swiper.js {% endcomment %}
{{ 'smi-swiper-bundle.min.css' | asset_url | stylesheet_tag }}
<script src="{{ 'smi-swiper-bundle.min.js' | asset_url }}" defer data-id="swiper"></script>
{% liquid
    assign space_between_mobile = settings.smi_spacing_grid_horizontal | at_most: 15
    assign space_between_desktop = settings.smi_spacing_grid_horizontal
    assign custom_blocks_image = section.blocks | where: 'type', 'image_block' | size
    assign custom_blocks_videos = section.blocks | where: 'type', 'video_block' | size
    assign block_length = custom_blocks_image | plus: custom_blocks_videos
    assign slides_per_view_desktop = section.settings.number_of_columns_on_desktop
    assign slides_per_view_laptop = 3 | at_most: slides_per_view_desktop
    assign slides_per_view_tablet = 2 | at_most: slides_per_view_desktop
    assign slides_per_view_mobile = section.settings.number_of_columns_on_mobile | at_most: block_length
    if section.settings.enable_swipe_on_mobile == true
        assign offset_swiper_mobile = 30
    else
        assign offset_swiper_mobile = 0
    endif
    if block_length == slides_per_view_mobile
        assign offset_swiper_mobile = 0
    endif
    if section.settings.content_alignment == 'left'
        assign classes_position_button = 'smi-justify-content-start'
    elsif section.settings.content_alignment == 'right'
        assign classes_position_button = 'smi-justify-content-end'
    else
        assign classes_position_button = 'smi-justify-content-center'
    endif
%}
<style>
    {% assign colorblock = section.blocks | where: 'type', 'color' | first %}
{% if colorblock %}
      #shopify-section-{{ section.id }},
      #shopify-section-{{ section.id }} .smi-color-{{ section.settings.color_scheme }},
      .smi-block-custom-color { 
        --color-background: {{ colorblock.settings.background_color.red }}, {{ colorblock.settings.background_color.green }},{{ colorblock.settings.background_color.blue }};        
        {% if colorblock.settings.background_gradient_color == blank %}
            --gradient-background: {{ colorblock.settings.background_color }};
        {% else %}
            --gradient-background: {{ colorblock.settings.background_gradient_color }};
        {% endif %}
        --color-foreground-hex: {{ colorblock.settings.text_color }};
        --color-foreground: {{ colorblock.settings.text_color.red }}, {{ colorblock.settings.text_color.green }},{{ colorblock.settings.text_color.blue }};
        --color-subtext: {{ colorblock.settings.subtext_color.red }},{{ colorblock.settings.subtext_color.green }},{{ colorblock.settings.subtext_color.blue }};
        --color-button: {{ colorblock.settings.solid_background_button_color.red }}, {{ colorblock.settings.solid_background_button_color.green }},{{ colorblock.settings.solid_background_button_color.blue }};
        --color-button-text: {{ colorblock.settings.solid_button_label_color.red }}, {{ colorblock.settings.solid_button_label_color.green }},{{ colorblock.settings.solid_button_label_color.blue }};
        --color-secondary-button-text: {{ colorblock.settings.outline_button_color.red }}, {{ colorblock.settings.outline_button_color.green }},{{ colorblock.settings.outline_button_color.blue }};
        --color-shadow: {{ colorblock.settings.shadow_color.red }}, {{ colorblock.settings.shadow_color.green }},{{ colorblock.settings.shadow_color.blue }};
    }
{% endif %}
    #shopify-section-{{ section.id }}{
        --section-padding-top: {{ section.settings.padding_top }}px;
--section-padding-bottom: {{ section.settings.padding_bottom }}px;
        --heading-alignment: {{ section.settings.heading_alignment }};
        --content-alignment: {{ section.settings.content_alignment }};
        --space-between-mobile: var(--grid-mobile-horizontal-spacing);
        --space-between-desktop: var(--grid-desktop-horizontal-spacing);
        {% comment %} --item-width-xs: {{ slides_per_view_mobile | minus: 1 | times: space_between_mobile | plus: offset_swiper_mobile | prepend: 'calc((100% - ' | append: 'px)/' | append: slides_per_view_mobile | append: ')'  }}; {% endcomment %}
        --item-width-xs: calc((100% - (({{ slides_per_view_mobile }} - 1) * var(--space-between-mobile) + {{ offset_swiper_mobile }}px)) / {{ slides_per_view_mobile }});
        {% comment %} --item-width-md: {{ slides_per_view_tablet | minus: 1 | times: space_between_desktop | prepend: 'calc((100% - ' | append: 'px)/' | append: slides_per_view_tablet | append: ')'  }}; {% endcomment %}
        --item-width-md: calc((100% - (({{ slides_per_view_tablet }} - 1) * var(--space-between-desktop))) / {{ slides_per_view_tablet }});
        {% comment %} --item-width-lg: {{ slides_per_view_laptop | minus: 1 | times: space_between_desktop | prepend: 'calc((100% - ' | append: 'px)/' | append: slides_per_view_laptop | append: ')'  }}; {% endcomment %}
        --item-width-lg: calc((100% - (({{ slides_per_view_laptop }} - 1) * var(--space-between-desktop))) / {{ slides_per_view_laptop }});
        {% comment %} --item-width-xl: {{ slides_per_view_desktop | minus: 1 | times: space_between_desktop | prepend: 'calc((100% - ' | append: 'px)/' | append: slides_per_view_desktop | append: ')'  }}; {% endcomment %}
        --item-width-xl: calc((100% - (({{ slides_per_view_desktop }} - 1) * var(--space-between-desktop))) / {{ slides_per_view_desktop }});
    }
</style>
<div id="smi-{{ section.id }}" class="smi-{{ section.id }} smi-section__inner smi-gradient smi-section-image-with-text-3 smi-color-{{ section.settings.color_scheme }}" data-section="image-with-text-3" data-name="✧Image with text 3">
      
    {% render 'smi-background' %}
    <div class="smi-container">
         {%-liquid
    if settings.smi_animations_reveal_on_scroll != nil
        assign animation_scroll = settings.smi_animations_reveal_on_scroll
    else
        assign global_settings = shop.metafields.smind.settings.value
        assign animation_scroll = global_settings.revealOnScroll
    endif
-%}
<div class="smi-heading-component smi-heading-component--align-{{section.settings.heading_alignment}} smi-link-wrapper" {{- block.shopify_attributes -}}    >
  {% liquid
    assign heading = section.settings.heading | strip
    assign sub_heading = section.settings.sub_heading | strip
    assign description = section.settings.description | strip
  %}
  <div class="smi-heading-inner
    {%- if animation_scroll %} smi-scroll-trigger smi-animate--slide-in{% endif -%}
    {%- if heading != blank or sub_heading != blank or description != blank %} smi-mb-xxl{% endif -%}
    ">
        {%- assign sub_heading = section.settings.sub_heading | strip -%}
{% if sub_heading != blank %}
    <p class="smi-text-sub smi-mb-xxs smi-heading-component__subheading">{{- sub_heading -}}</p>
{% endif %} 
        {%- assign description = section.settings.description | strip -%}
{%- assign heading = section.settings.heading | strip -%}
{% if heading != blank %}
    <h2 class="{{ section.settings.heading_size }} {% if description != blank %}smi-mb-sm{% endif %} smi-heading-component__heading">{{- heading -}}</h2>
{% endif %} 
        {%- assign description = section.settings.description | strip -%}
{% if description != blank %}
    <div class="smi-text-md smi-heading-component__description">{{ description }}</div>
{% endif %}
    </div>
</div>
    </div>
    {% comment %}Swiper Slide{% endcomment %}
    {%-liquid
    if settings.smi_animations_reveal_on_scroll != nil
        assign animation_scroll = settings.smi_animations_reveal_on_scroll
    else
        assign global_settings = shop.metafields.smind.settings.value
        assign animation_scroll = global_settings.revealOnScroll
    endif
-%}
    <div class="smi-section-image-with-text-3__main {% if section.settings.enable_swipe_on_mobile == true %}smi-swipe-on-mobile{% else %}smi-grid-on-mobile{% endif %}">
        <div class="{% if section.settings.container_full_width %}smi-container-fluid{% else %}smi-container{% endif -%} {%- if animation_scroll %} smi-scroll-trigger smi-animate--slide-in smi-animate-order-1{% endif -%}">
            <div class="swiper smi-swiper" data-swiper-options='{"spaceBetween":{{ space_between_mobile }},"pageDotStyle":"text-style-2"}'>
                <div class="swiper-wrapper">
                    {% assign shape_index = 1 %}
                    {% for block in section.blocks %}
                        {% if block.type == 'video_block' or block.type == 'image_block' %}
                            {%-liquid
    assign media_ratio = section.settings.media_ratio
    assign media_shape = section.settings.media_shape
    assign media_classes = 'smi-w-100 smi-object-fit-cover '
    if media_shape != 'default' and media_shape != 'round'
        if shape_index > 6
            assign shape_index = 1
        endif
        if shape_index < 6 
            assign forloop_next = shape_index | plus: 1
        else
            assign forloop_next = 1
        endif
        assign media_classes = media_classes | append: ' smi-shape-blob-' | append: shape_index
        assign shape_index = shape_index | plus: 1
        assign shape_hover = '--shape-blob-' | append: forloop_next
    endif
    if media_ratio == 'auto' 
        assign media_classes = media_classes | append: ' smi-h-auto'
    else
        assign media_classes = media_classes | append: ' smi-h-100 smi-p-absolute'
    endif
    assign media_wrapper_classes = 'smi-media-wrapper smi-border smi-shadow smi-lh-0 smi-overflow-hidden smi-p-relative smi-' | append: media_shape
    if media_shape contains 'blob'
        assign media_wrapper_classes = media_wrapper_classes | append: ' smi-blob-wrapper smi-has-blob'
    endif
-%}
                            <div class="swiper-slide" data-swiper-slide-index="{{ forloop.index0 }}" {{ block.shopify_attributes }}>
                                <div class="card-collection-wrap">
                                    {%- liquid
    assign author_name = block.settings.author_name | strip
    assign author_info = block.settings.author_info | strip
    assign verified_buyer = block.settings.verified_buyer
    assign nation = block.settings.nation
    assign heading = block.settings.heading | strip
    assign description = block.settings.description | strip
    assign image = block.settings.image
    assign product = block.settings.product
    assign has_product = ''
    if product != blank
        assign has_product = 'smi-has-product'
    endif
-%}
{% assign img_classes = media_classes %}
<div class="smi-item smi-d-flex {{- ' smi-hover-bg-image-' | append: settings.smi_animations_hover_image | default: shop.metafields.smind.settings.value.imageHoverEffect | default: 'none' }}">
    {%- if block.type == 'image_block' -%}
        <div class="smi-item__media smi-lh-0 smi-media-wrapper smi-media {{ media_wrapper_classes }} {{ settings.smi_animations_hover_image | default: shop.metafields.smind.settings.value.imageHoverEffect | default: 'none' | prepend: ' smi-hover-image-' }}" 
            style="--media-ratio: {{ media_ratio }}; --shape-hover: var({{ shape_hover }})">
            {% comment %} <div class="smi-cover-image"
                onclick="smiLiteModal.open('#modal_{{section.id}}{{block.id}}')"> {% endcomment %}
                {% assign img_classes = media_classes | append: 'smi-default-image' %}
                {%- if image != blank -%}
                    {{ image | image_url: width: 800 | image_tag: loading: 'lazy', class: media_classes }}
                {% else %}
                {{ 'hero-apparel-3' | placeholder_svg_tag: media_classes }}
                {%- endif -%}
            {% comment %} </div> {% endcomment %}
        </div>
    {%- elsif block.type == 'video_block' -%}
    <div class="smi-item__media smi-lh-0 smi-autoplay-{{ block.settings.autoplay }} smi-media-wrapper smi-media-wrapper-{{ section.id }}-{{ forloop.index }} smi-media {{ media_wrapper_classes }}" style="--media-ratio: {% if block.settings.autoplay == true and media_ratio == 'auto' %}4/3{% else %}{{ media_ratio }}{% endif %};; --shape-hover: var({{ shape_hover }});">
            {%- if block.settings.autoplay == true -%}
                {% assign has_video = false %}
                {% if block.settings.video != blank %}
                    {% assign has_video = true %}
                    {% assign smi_bg_vi_cus = media_classes | append: " smi-background-video" %}
                    {{ block.settings.video | video_tag: autoplay: true, loop: true, muted: true, controls: false, image_size: '1100x', class: smi_bg_vi_cus }}
                {% elsif block.settings.video_url != blank %}
                    {% assign has_video = true %}
                    <div class="smi-video-url smi-background-block {% if block.settings.autoplay == true %} smi-background-video {% endif -%} {{ media_classes }}">
                        {%- if block.settings.video_url.type == 'youtube' -%}
                            <iframe
                                    src="https://www.youtube.com/embed/{{ block.settings.video_url.id }}?enablejsapi=1
                                        {%- if block.settings.autoplay == true %}&autoplay=1&mute=1&loop=1&controls=0{% endif -%}"
                                    class="smi-js-youtube "
                                    allow="autoplay; encrypted-media" loading="lazy"
                                    allowfullscreen
                                    title="{{ block.settings.description | escape }}"
                                    width="100%"
                                    height="100%"
                            ></iframe>
                        {%- else -%}
                            <iframe
                                    src="https://player.vimeo.com/video/{{ block.settings.video_url.id }}?title=0&byline=0&portrait=0
                                        {%- if block.settings.autoplay == true %}&autoplay=1&loop=1&muted=1{% endif -%}"
                                    class="smi-js-vimeo"
                                    allow="autoplay; encrypted-media" loading="lazy"
                                    allowfullscreen
                                    title="{{ block.settings.description | escape }}"
                                    width="100%"
                                    height="100%"
                            ></iframe>
                        {%- endif -%}
                    </div>
                {% else %} 
                {% assign img_classes = img_classes | append: 'smi-default-image' %}
                    {{ 'hero-apparel-3' | placeholder_svg_tag: img_classes }}
                {% endif %}
                {% if has_video %} 
                    <div class="smi-volume">
                        {% render 'smi-icons', icon: 'volumn-up' %}
                        {% render 'smi-icons', icon: 'volumn-mute' %}
                    </div>
                {% endif %}
            {%- else -%}
                <div class="smi-cover-image {{ settings.smi_animations_hover_image | default: shop.metafields.smind.settings.value.imageHoverEffect | default: 'none' | prepend: ' smi-hover-image-' }}" 
                    onclick="smiLiteModal.open('#modal_{{section.id}}{{block.id}}')">
                    <div class="smi-cover-image__play-button">
                        {% render 'smi-icons', icon: 'play-button' %}
                    </div>
                    {%- if block.settings.cover_image != blank -%}                        
                    {{ block.settings.cover_image | image_url: width: 800 | image_tag: loading: 'lazy', class: img_classes }}
                    {%- else -%}
                    {% assign img_classes = img_classes | append: 'smi-default-image' %}
                    {{ 'hero-apparel-3' | placeholder_svg_tag: img_classes }}
                    {%- endif -%}
                </div>
                <div class="smi-video-modal smi-lite-modal smi-section smi-dropdown-popup-wrapper smi-border smi-shadow smi-modal-xl" id="modal_{{section.id}}{{block.id}}">
                    <button onclick="smiLiteModal.close()" class="smi-modal-close">{% render 'smi-icons', icon: 'close' %}</button>
                    {% if block.settings.video != blank %}
                        <div class="smi-upload-video">
                            {{ block.settings.video | video_tag: autoplay: false, loop: true, muted: false, controls: true, image_size: '1100x' }}
                        </div>
                    {% elsif block.settings.video_url != blank %}
                    <div class="smi-video-url smi-background-block">
                        {%- if block.settings.video_url.type == 'youtube' -%}
                            <iframe
                                src="https://www.youtube.com/embed/{{ block.settings.video_url.id }}?enablejsapi=1&autoplay=0&mute=1&loop=0&controls=1&playlist={{ block.settings.video_url.id }}&rel=0"
                                class="smi-js-youtube"
                                allow="autoplay; encrypted-media" loading="lazy"
                                allowfullscreen
                                width="100%"
                                height="100%"
                            ></iframe>
                        {%- else -%}
                            <iframe
                                src="https://player.vimeo.com/video/{{ block.settings.video_url.id }}?autoplay=0&pip=0&muted=1"
                                class="smi-js-vimeo"
                                allow="autoplay; encrypted-media" loading="lazy"
                                allowfullscreen
                                width="100%"
                                height="100%"
                            ></iframe>
                        {%- endif -%}
                    </div>
                    {% else %}
                    {% assign img_classes = img_classes | append: 'smi-default-image' %}
                    {{ 'hero-apparel-3' | placeholder_svg_tag: img_classes }}
                    {% endif %}
                </div>
            {%- endif -%}
        </div>
    {%- endif -%}
    <div class="smi-item-content smi-pt-lg
        {% if section.settings.content_alignment == "right" %}
            smi-pl-md
        {% elsif section.settings.content_alignment == "left" %}
            smi-pr-md
        {% else %}
        smi-px-md
        {% endif %}
        smi-content-align-{{ section.settings.content_alignment }}
         {% comment %} smi-gradient smi-color-{{ section.settings.content_color_scheme }} {% endcomment %}
         ">
         {% if block.settings.heading != blank %}
            <div class="smi-item-content-heading {{ block.settings.heading_size }}">
                {{  block.settings.heading }}
            </div>
        {%  endif %}
        {% if block.settings.content != blank %}
            <div class="smi-item-content-detail smi-text-75 smi-mt-xxs smi-text-md">
                {{  block.settings.content }}
            </div>
        {%  endif %}
        {% if block.settings.button_label != blank %}
            <div class="smi-component-buttons smi-mt-md smi-button-wrapper smi-component-button-{{ section.settings.content_alignment }}">
            <a href="{% if block.settings.button_label != blank %} {{ block.settings.button_link }} {% else %}javascript:; {% endif %}" 
                class="smi-button smi-text-button smi-button--text
                {%- if button_style contains 'arrow' %} smi-has-icon{% endif %}
                {{- settings.smi_animations_hover_elements | default: shop.metafields.smind.settings.value.buttonHoverEffect | default: 'none' | prepend: ' smi-hover-' -}}
                {{- ' smi-border smi-shadow' -}}">
            <span>
                {{- block.settings.button_label | escape -}}
            </span>
            </a>
        </div>
        {%  endif %}
    </div>
    {% comment %} <div class="smi-item-content smi-py-lg smi-px-lg smi-content-align-{{ section.settings.content_alignment }} {{ has_product }} smi-gradient smi-color-{{ section.settings.content_color_scheme }}">
        <div>
        <div class="smi-item__author smi-mb-sm">
            <div class="smi-name smi-d-flex smi-align-items-center smi-flex-wrap smi-column-gap-xs smi-link-wrapper">
                {%- if author_name != '' -%}
                    <h5 class="smi-h5 smi-color-text">{{- author_name -}}</h5>
                {%- endif -%}
                {%- if verified_buyer != 'none' -%}
                    <p class="smi-name__verified smi-d-flex smi-align-items-center smi-column-gap-xxs">
                        {% render 'smi-icons', icon: 'verify' %}
                        {%- if verified_buyer == 'icon_with_text' -%}
                            {% assign percent = '10%' %}
                            <span clas="smi-text-md">{{ 'smind.general.verified_buyer' | t }}</span>
                        {%- endif -%}
                    </p>
                {%- endif -%}
            </div>
            <div class="smi-info smi-d-flex smi-align-items-center smi-column-gap-xs smi-link-wrapper">
                {%- if author_info != '' -%}
                    <p class="smi-text-md smi-color-text-75">{{- author_info -}}</p>
                {%- endif -%}
                {% render 'smi-flags', icon: nation %}
            </div>
        </div>
        <div class="smi-mb-sm">
{%- if block.settings.star != 'none' -%}
    <div class="smi-rating-component smi-lh-0">
        <div class="smi-d-inline-flex smi-align-items-center smi-column-gap-xxs smi-rating-component-inner smi-rating-inner--small">
