    {% for i in (1..5) %}
       {%- if i <= star_floor -%}
            <span>{% render 'smi-icons', icon: 'star-2' %}</span>
        {% elsif i > star_floor and i <= star_ceil %}
            <span>{% render 'smi-icons', icon: 'star-half' %}</span>
